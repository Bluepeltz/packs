{
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ],
      "include": [
        "prefer-ascii",
        "camelcase",
        "eqeqeq",
        "no-const-assign",
        "no-eval",
        "no-non-null-assertion",
        "no-throw-literal",
        "no-sparse-arrays",
        "no-external-import"
      ]
    },
    "exclude": [
      "_fresh"
    ]
  },
  "fmt": {
    "useTabs": false,
    "singleQuote": true,
    "indentWidth": 2,
    "exclude": [
      "_fresh"
    ]
  },
  "lock": false,
  "tasks": {
    "start": "deno run -A --unstable --watch=static/,routes/,islands/,components/,utils/,i18n/ dev.ts",
    "build": "deno run -A --unstable dev.ts build",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "preview": "deno run -A --unstable main.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "#filter-boolean": "https://esm.sh/@total-typescript/ts-reset@0.4.2/filter-boolean",
    "$fresh/": "https://deno.land/x/fresh@1.4.2/",
    "$std/": "https://deno.land/std@0.198.0/",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "kv_oauth/": "https://deno.land/x/deno_kv_oauth@v0.6.0/",
    "bson": "https://raw.githubusercontent.com/lucsoft/web_bson/v0.3.0/mod.js",
    "icons/": "https://deno.land/x/tabler_icons_tsx@0.0.4/tsx/"
  }
}
